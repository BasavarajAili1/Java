<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


	<bean id="trainer1" class="com.kodnest.entities.Trainer">
		<property name="id" value="100" />
		<property name="name" value="Anthony" />
		<property name="salary" value="50000" />
	</bean>
	
	<bean id="university1" class="com.kodnest.entities.University">
		<property name="id" value="0001" />
		<property name="name" value="VTU" />
		<property name="trainer" ref="trainer1" />
	</bean>
	
	
</beans>

