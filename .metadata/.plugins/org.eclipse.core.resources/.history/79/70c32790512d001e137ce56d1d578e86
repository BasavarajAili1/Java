package com.kodnest.patternprograms.level5;

public class Pattern3 {

	public static void main(String[] args) {
		for(int i=3;i<=3;i++)
		{
			for(int j=1;j<=5;j++)
			{
				if((i==3 && j%1==0) || (i+j==4))
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
}
}
