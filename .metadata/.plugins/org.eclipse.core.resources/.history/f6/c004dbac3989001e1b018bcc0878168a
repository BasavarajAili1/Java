package com.kodnest.wrapperClasses;

import java.util.ArrayList;

public class WrapperDemo {

	public static void main(String[] args) {
		ArrayList<Integer> list1 = new ArrayList<Integer>();
		
		// Auto boxing
		list1.add(10); // list1.add(new Integer(10)
		list1.add(20); // list1.add(new Integer(20)
		list1.add(30); // list1.add(new Integer(30)
		System.out.println("ELements: "+list1);
		
		// Auto Unboxing
		int i1 = list1.get(0); // (int) list1.get(0)
		int i2 = list1.get(1); // (int) list1.get(1)
	}

}
