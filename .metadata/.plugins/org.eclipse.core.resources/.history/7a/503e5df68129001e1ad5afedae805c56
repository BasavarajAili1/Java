package mathclass;
import java.util.Scanner;

public class MathClassExamples {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("Enter two Numbers");
		int a=scan.nextInt();
		int b=scan.nextInt();
		maxOfTwoNum(a,b);
		minOfTwoNum(a,b);
	}
	public static void maxOfTwoNum(int a, int b) {
		System.out.println("Maximum of a and b is: "+Math.max(a, b));
	}
	public static void minOfTwoNum(int a, int b) {
		System.out.println("Minimum of a and b Numbers is: "+Math.min(a, b));
	}
	public static void sqrt(int a) {
		System.out.println("Square-root of a is: "+Math.sqrt(a));
	}
	public static void pow(int a, int b) {
		System.out.println("Power of a to b Numbers is: "+Math.pow(a, b));
	}
}
