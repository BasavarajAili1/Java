package patternProgramsPractice;

public class Pattern18 {
	//NUMBER TRIANGLE PATTERN
	public static void main(String[] args) {
		int n=6;
		for (int i=1;i<=n;i++) 
		{
			for (int j=1;j<=n-1;j++)
			{
				System.out.print(" ");
			}
			for (j=1;j<=i;j++)
			{
				System.out.print(i+" ");
			}
			System.out.println();
		}

	}

}
