package com.kodnest.mavenirDrive;
import java.util.Scanner;
import java.lang.Math;
public class FindPower {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("Enter Base and Exponential");
		int base=scan.nextInt();
		int exponential=scan.nextInt();
		long power=findPower(base, exponential);
		System.out.println(power);
	}
	public static long findPower(int base, int exponential) {
		return (long)Math.pow(base, exponential);
	}
}
