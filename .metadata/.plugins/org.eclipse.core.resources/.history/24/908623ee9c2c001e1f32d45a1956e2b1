package com.kodnest.patternprograms.level5;

public class Pattern1 {

	public static void main(String[] args) {
		for(int i=1;i<=3;i++)
		{
			if(i==1 || i==3)
			{
				for(int j=1;j<=3;j++) 
				{
					System.out.print("*");
				}
			}System.out.println();
			
			System.out.print("* *");
			
		}
	}

}
